# .env.example
# Copy to `.env` and fill in your own secret and config values

# Database connection URL (if you use any DB for metadata or checkpoints)
DATABASE_URL=postgresql://username:password@hostname:port/dbname

# API keys or tokens for external services (if applicable)
API_KEY=your_api_key_here

# Logging level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Email or alerting service API key for notifications (optional)
ALERTS_API_KEY=your_alerts_api_key_here

# Path overrides (optional, if you want to override config YAML paths)
INPUT_PATH=./data/raw/
BRONZE_PATH=./data/bronze/
SILVER_PATH=./data/silver/
GOLD_PATH=./data/gold/

# Any other secrets or environment variables your pipeline requires
SECRET_KEY=your_secret_here
